<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1758675279821-1" author="jmiranda">
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="requisition" columnName="priority"/>
            </not>
        </preConditions>
        <addColumn tableName="requisition">
            <column name="priority" type="INT">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>

    <changeSet id="1758675279821-2" author="jmiranda">
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="requisition" columnName="delivery_type_code"/>
            </not>
        </preConditions>
        <addColumn tableName="requisition">
            <column name="delivery_type_code" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>


    <changeSet id="1758675279821-3" author="jmiranda">
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="requisition" columnName="order_type_code"/>
            </not>
        </preConditions>
        <addColumn tableName="requisition">
            <column name="order_type_code" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>


</databaseChangeLog>